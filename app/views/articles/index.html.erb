<h1>Articles listing page</h1>
<table>
    <thead>
        <tr>
            <th>Title </th>

            <th>Description </th>

            <th>Actions </th>
        </tr>
    </thead>
    <tbody>
        <% @all_articles.each do |a| %>
            <tr>
                <td><%= a.title %></td>

                <td><%=a.description %> </td>

                <td>Actions </td>
            </tr>
        <% end %>
    </tbody>
</table>